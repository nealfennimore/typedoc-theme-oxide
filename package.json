  "main": "dist/index.js",
  "repository": "github:nealfennimore/typedoc-theme-oxide",
  "author": "Balthild <ibalthild@gmail.com>",
  "license": "MIT",
  "keywords": [
    "rustdoc",
    "typedoc",
    "typedoc-theme",
    "typescript"
